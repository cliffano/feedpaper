{
  "commands": {
    "start": {
      "desc": "Start FeedPaper application",
      "options": [
        { "arg": "-f, --feeds-file <feedsFile>", "desc": "Feeds setting file | default: conf/feeds.json" },
        { "arg": "-c, --conf-dir <confDir>", "desc": "Applications configuration directory | default: conf" },
        { "arg": "-d, --cache-db-dir <cacheDbDir>", "desc": "Cache database directory | default: /tmp/feedpaper.db" }
      ],
      "examples": [
        "feedpaper start",
        "feedpaper start --feeds-file /path/to/somefeedfile.json",
        "feedpaper start --conf-dir /path/to/someconfdir"
      ]
    }
  }
}
